            {
               "conditions": [
                  {
                     "bundle_identifiers": [
                        "^org\\.gnu\\.emacs$",
                        "^com\\.jetbrains",
                        "^com\\.microsoft\\.VSCode$",
                        "^com\\.sublimetext\\.3$"
                     ],
                     "type": "frontmost_application_if"
                  }
               ],
               "from": {
                  "key_code": "c",
                  "modifiers": {
                     "mandatory": [
                        "control"
